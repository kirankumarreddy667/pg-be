<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <style>
        table,
        table th,
        table td {
            text-align: center;
        }
    </style>
    <title>Milk Production Quality</title>
</head>
<body>
    <div class="container">
        <center><h1>Milk Production Quality</h1></center>
        <br>
        
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Morning Fat</th>
                    <th>Evening Fat</th>
                    <th>Morning SNF</th>
                    <th>Evening SNF</th>
                </tr>
            </thead>
            <tbody>
                <% if (milkProductionQualityData && milkProductionQualityData.length > 0) { %>
                    <% milkProductionQualityData.forEach(function(value) { %>
                        <tr>
                            <td><%= value.date %></td>
                            <td><%= value.totalMorningFat %></td>
                            <td><%= value.totaleveningFat %></td>
                            <td><%= value.totalMorningSNF %></td>
                            <td><%= value.totaleveningSNF %></td>
                        </tr>
                    <% }); %>
                <% } else { %>
                    <tr>
                        <td colspan="5">No milk production quality data available</td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</body>
</html>